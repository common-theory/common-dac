{
  "name": "@ctheory/voting-logic-store",
  "version": "0.0.1",
  "description": "The common theory dac smart contract",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build",
    "build": "npm run clean && truffle compile",
    "deploy:dev": "npm run clean && truffle migrate --network development",
    "deploy:rinkeby": "npm run clean && truffle migrate --network rinkeby",
    "ganache": "ganache-cli -b 10 -p 7545",
    "coverage": "solidity-coverage && lcov-badge-generator ./coverage/lcov.info -o ./test/badge.svg",
    "upload-coverage": "jsdnslink update _dnslink.coverage.commontheory.io /ipfs/$(ipfs add -Qr ./coverage)",
    "precommit": "npm run coverage && git add ./test/badge.svg",
    "test": "truffle test"
  },
  "author": "Chance Hudson",
  "license": "MIT",
  "devDependencies": {
    "digitalocean": "^1.2.0",
    "dnslink": "^0.2.0",
    "ganache-cli": "^6.1.8",
    "git-pre-commit": "^2.1.4",
    "lcov-badge-generator": "^1.0.5",
    "node-fetch": "^2.2.0",
    "solidity-coverage": "^0.5.11",
    "truffle": "^4.1.14",
    "truffle-hdwallet-provider": "0.0.6"
  },
  "dependencies": {},
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/common-theory/voting-logic.git"
  },
  "bugs": {
    "url": "https://github.com/common-theory/voting-logic/issues"
  },
  "homepage": "https://github.com/common-theory/voting-logic#readme"
}
